{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Assignment 4 template for creating VPC, Internet Gateway, Route tables and Subnets",
    "Parameters": {
        "VPCName": {
            "Type": "String"
        },
        "InternetGatewayName": {
            "Type": "String"
        },
        "PublicRouteTableName": {
            "Type": "String"
        },
        "PrivateRouteTableName": {
            "Type": "String"
        },
        "WebServerSubnet1Name": {
            "Type": "String"
        },
        "WebServerSubnet2Name": {
            "Type": "String"
        },
       "WebServerSubnet3Name": {
            "Type": "String"
        },
        "DBServerSubnetName": {
            "Type": "String"
        },
        "DBServerSubnet2Name": {
            "Type": "String"
        },
        "DBServerSubnet3Name": {
            "Type": "String"
        }
    },
    "Resources": {
        "myVPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "10.0.0.0/16",
                "InstanceTenancy": "default",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "VPCName"
                        }
                    }
                ]
            }
        },
        "myInternetGateway": {
            "Type": "AWS::EC2::InternetGateway",
            "Properties": {
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "InternetGatewayName"
                        }
                    }
                ]
            }
        },
        "AttachInternetGateway": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "InternetGatewayId": {
                    "Ref": "myInternetGateway"
                },
                "VpcId": {
                    "Ref": "myVPC"
                }
            }
        },
        "myPublicRouteTable": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "myVPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "PublicRouteTableName"
                        }
                    }
                ]
            }
        },
        "myPublicRoute": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "GatewayId": {
                    "Ref": "myInternetGateway"
                },
                "RouteTableId": {
                    "Ref": "myPublicRouteTable"
                }
            }
        },
        "myPrivateRouteTable": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "myVPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "PrivateRouteTableName"
                        }
                    }
                ]
            }
        },
        "myWebServerSubnet1": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1a",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.0.0/24",
                "MapPublicIpOnLaunch": "true",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "WebServerSubnet1Name"
                        }
                    }
                ]
            }
        },
        "myWebServerSubnetRouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myWebServerSubnet1"
                },
                "RouteTableId": {
                    "Ref": "myPublicRouteTable"
                }
            }
        },


        "myWebServerSubnet2": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1c",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.2.0/24",
                "MapPublicIpOnLaunch": "true",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "WebServerSubnet2Name"
                        }
                    }
                ]
            }
        },
        "myWebServerSubnet2RouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myWebServerSubnet2"
                },
                "RouteTableId": {
                    "Ref": "myPublicRouteTable"
                }
            }
        },
      
       "myWebServerSubnet3": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1e",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.4.0/24",
                "MapPublicIpOnLaunch": "true",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "WebServerSubnet3Name"
                        }
                    }
                ]
            }
        },
        "myWebServerSubnet3RouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myWebServerSubnet3"
                },
                "RouteTableId": {
                    "Ref": "myPublicRouteTable"
                }
            }
        },
       "myDBServerSubnet": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1b",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.1.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "DBServerSubnetName"
                        }
                    }
                ]
            }
        },
        "myDBServerSubnetRouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myDBServerSubnet"
                },
                "RouteTableId": {
                    "Ref": "myPrivateRouteTable"
                }
            }
        },
       "myDBServerSubnet2": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1d",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.3.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "DBServerSubnet2Name"
                        }
                    }
                ]
            }
        },

        
        "myDBServer2SubnetRouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myDBServerSubnet2"
                },
                "RouteTableId": {
                    "Ref": "myPrivateRouteTable"
                }
            }
        },
        "myDBServerSubnet3": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "AvailabilityZone" : "us-east-1f",
                "VpcId": {
                    "Ref": "myVPC"
                },
                "CidrBlock": "10.0.5.0/24",
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "DBServerSubnet3Name"
                        }
                    }
                ]
            }
        },
        "myDBServer3SubnetRouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "myDBServerSubnet3"
                },
                "RouteTableId": {
                    "Ref": "myPrivateRouteTable"
                }
            }
        }
        



        
    }
}

